    "@lottiefiles/react-lottie-player": "^3.5.2",
    "@next/font": "13.1.6",
    "dayjs": "^1.11.7",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "fontawesome": "^5.6.3",
    "framer-motion": "^10.2.3",
    "lottie-react": "^2.4.0",
    "next": "13.1.6",
    "react": "18.2.0",
