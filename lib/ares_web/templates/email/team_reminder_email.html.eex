              following link:<!-- -->
              <a
                href=<%= "#{@phx_host}/app/team-formation" %>
                style="color:rgb(51,65,85)"
                ><%= "#{@phx_host}/app/team-formation" %></a
              >
            </p>
          </td>
        </tr>
      </tbody>
    </table>
    <!--/$-->
